<Window x:Class="Repair.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Repair"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Style="{StaticResource BaseWindowStyle}">
    <Grid>
        <Frame Content="Frame" Name="MainFrame" Source="Pages\DataPage.xaml" Margin="0,52,0,0" NavigationUIVisibility="Hidden"/>
        <Button Content="Назад" Style="{StaticResource BaseButtonStyle}" Click="Button_Back_Click" HorizontalAlignment="Left"  VerticalAlignment="Top" Height="37" Width="129" FontSize="24"/>

    </Grid>
</Window>
